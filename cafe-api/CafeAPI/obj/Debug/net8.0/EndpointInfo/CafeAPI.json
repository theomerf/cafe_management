        },
        "additionalProperties": false
      },
      "TableStatus": {
        "enum": [
          "Occupied",
          "Free",
          "OutOfOrder"
        ],
        "type": "string"
      },
      "TokenDto": {
        "type": "object",
        "properties": {
          "accessToken": {
            "type": "string",
            "nullable": true
          },
          "refreshToken": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "apiKey",
        "description": "Please enter a valid token",
