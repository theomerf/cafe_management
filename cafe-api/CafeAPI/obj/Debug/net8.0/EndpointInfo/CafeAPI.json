  "components": {
    "schemas": {
      "AccountDtoForLogin": {
        "required": [
          "password",
          "userName"
        ],
        "type": "object",
        "properties": {
          "userName": {
            "minLength": 3,
            "type": "string"
          },
          "password": {
            "minLength": 6,
            "type": "string"
          },
          "rememberMe": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "AccountDtoForRegistration": {
        "required": [
          "email",
          "firstName",
          "lastName",
          "password",
          "phoneNumber",
          "roles",
          "userName"
        ],
        "type": "object",
        "properties": {
          "firstName": {
            "maxLength": 20,
            "minLength": 2,
            "type": "string"
          },
          "lastName": {
            "maxLength": 20,
            "minLength": 2,
            "type": "string"
          },
          "userName": {
            "maxLength": 20,
            "minLength": 3,
            "type": "string"
          },
          "password": {
            "maxLength": 20,
            "minLength": 1,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string"
          },
          "phoneNumber": {
            "minLength": 1,
            "type": "string"
          },
          "roles": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "additionalProperties": false
      },
      "OrderDtoForCreation": {
        "required": [
          "orderLines"
        ],
        "type": "object",
        "properties": {
          "totalAmount": {
            "type": "number",
            "format": "double"
          },
          "orderLines": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/OrderLineDto"
            }
          },
          "tableId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "OrderDtoForUpdate": {
        "required": [
          "orderLines"
        ],
        "type": "object",
        "properties": {
          "totalAmount": {
            "type": "number",
            "format": "double"
          },
          "orderLines": {
            "type": "array",
